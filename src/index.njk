        {% else %}
            <h1>NO PROJECT DATA</h1>
        {% endfor %}
    </div>
    <hr />

    <h2 class="category-header" id="programs">Programs</h2>
    <div class="project-container">
        {% for proj in data.categories.programs %}
            {{ project.new(proj) }}
        {% else %}
            <h1>NO PROJECT DATA</h1>
        {% endfor %}
    </div>
    <hr />

    <h2 class="category-header" id="addons">AddOns</h2>
