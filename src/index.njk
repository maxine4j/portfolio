{% extends "_layout.njk" %}
{% import "macros/_project.njk" as project %}

{% block head %}
<title>Tim Ings</title>
{% endblock %}

{% block content %}
    <div class="page-title">
        <h1 class="typewriter">tim-ings $&nbsp<input id="cmd" maxlength="16" autocomplete="off" autocorrect="off" autocapitalize="off" spellcheck="false" /></h1>
    </div>

    <h2 class="category-header" id="featured">Featured</h2>
    <div class="project-container">
        {% for proj in data.categories.featured %}
            {{ project.new(proj) }}
        {% else %}
            <h1>NO PROJECT DATA</h1>
        {% endfor %}
    </div>
    <hr />

    <h2 class="category-header" id="websites">Websites</h2>
    <div class="project-container">
        {% for proj in data.categories.websites %}
            {{ project.new(proj) }}
